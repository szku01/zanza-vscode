{
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.minimap.showSlider": "always",
  "editor.rulers": [ { "column": 0, "color": "#404040" }, { "column": 100, "color": "#88888830" }, 120 ],
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "eslint.alwaysShowStatus": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmUndo": "light",
  "explorer.openEditors.visible": 20,
  "extensions.ignoreRecommendations": true,
  "files.enableTrash": false,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.blame.avatars": false,
  "gitlens.blame.format": "${author|15} ${message|30} ${agoOrDate|14-}",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.defaultDateFormat": "YYYY-MM-DD",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.defaultTimeFormat": "HH:mm",
  "gitlens.graph.statusBar.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.views.commits.avatars": false,
  "gitlens.views.patchDetails.avatars": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.shellIntegration.enabled": false,
  "workbench.editor.showTabs": "single",
  "workbench.tree.enableStickyScroll": false
}
