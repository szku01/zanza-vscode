{
  "name": "zanza",
  "displayName": "Zanza",
  "description": "simple things I miss from vscode",
  "publisher": "szkrd",
  "repository": "https://github.com/szkrd/zanza-vscode",
  "version": "5.0.4",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "iconThemes": [
      {
        "id": "zanza",
        "label": "Zanza (minimal modified)",
        "path": "./icons/zanza-icon-theme.json"
      }
    ],
    "keybindings": [
      {
        "command": "zanza.saveBookmark1",
        "key": "alt+shift+d 1"
      },
      {
        "command": "zanza.loadBookmark1",
        "key": "alt+d 1"
      },
      {
        "command": "zanza.saveBookmark2",
        "key": "alt+shift+d 2"
      },
      {
        "command": "zanza.loadBookmark2",
        "key": "alt+d 2"
      },
      {
        "command": "zanza.saveBookmark3",
        "key": "alt+shift+d 3"
      },
      {
        "command": "zanza.loadBookmark3",
        "key": "alt+d 3"
      },
      {
        "command": "zanza.saveBookmark4",
        "key": "alt+shift+d 4"
      },
      {
        "command": "zanza.loadBookmark4",
        "key": "alt+d 4"
      },
      {
        "command": "zanza.saveBookmark5",
        "key": "alt+shift+d 5"
      },
      {
        "command": "zanza.loadBookmark5",
        "key": "alt+d 5"
      },
      {
        "command": "zanza.saveBookmark6",
        "key": "alt+shift+d 6"
      },
      {
        "command": "zanza.loadBookmark6",
        "key": "alt+d 6"
      },
      {
        "command": "zanza.saveBookmark7",
        "key": "alt+shift+d 7"
      },
      {
        "command": "zanza.loadBookmark7",
        "key": "alt+d 7"
      },
      {
        "command": "zanza.saveBookmark8",
        "key": "alt+shift+d 8"
      },
      {
        "command": "zanza.loadBookmark8",
        "key": "alt+d 8"
      },
      {
        "command": "zanza.loadBookmarkFromPicker",
        "key": "alt+d alt+d"
      },
      {
        "command": "zanza.saveBookmarkToPicker",
        "key": "alt+shift+d alt+shift+d"
      },
      {
        "command": "zanza.flushBookmarks",
        "key": "alt+shift+d x"
      },
      {
        "command": "zanza.copyToBuffer",
        "key": "ctrl+c",
        "when": "editorFocus",
        "mac": "cmd+c"
      },
      {
        "command": "zanza.cutToBuffer",
        "key": "ctrl+x",
        "when": "editorFocus && !editorReadonly",
        "mac": "cmd+x"
      },
      {
        "command": "zanza.pasteFromBuffer1",
        "key": "ctrl+shift+v 1",
        "when": "editorTextFocus && !editorReadonly",
        "mac": "cmd+shift+v 1"
      },
      {
        "command": "zanza.pasteFromBuffer2",
        "key": "ctrl+shift+v 2",
        "when": "editorTextFocus && !editorReadonly",
        "mac": "cmd+shift+v 2"
      },
      {
        "command": "zanza.pasteFromBuffer3",
        "key": "ctrl+shift+v 3",
        "when": "editorTextFocus && !editorReadonly",
        "mac": "cmd+shift+v 3"
      },
      {
        "command": "zanza.pasteFromBuffer4",
        "key": "ctrl+shift+v 4",
        "when": "editorTextFocus && !editorReadonly",
        "mac": "cmd+shift+v 4"
      },
      {
        "command": "zanza.pasteFromBuffer5",
        "key": "ctrl+shift+v 5",
        "when": "editorTextFocus && !editorReadonly",
        "mac": "cmd+shift+v 5"
      },
      {
        "command": "zanza.showClipboardBuffer",
        "key": "ctrl+shift+v ctrl+shift+v",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "zanza.commentDown",
        "key": "ctrl+shift+/",
        "when": "editorTextFocus && !editorReadonly",
        "mac": "cmd+alt+/"
      },
      {
        "command": "zanza.fromDiffToFile",
        "key": "f4",
        "when": "isInDiffEditor"
      },
      {
        "command": "zanza.greedyDeleteLeft",
        "key": "ctrl+shift+backspace",
        "when": "editorTextFocus && !editorReadonly",
        "mac": "cmd+shift+backspace"
      },
      {
        "command": "zanza.openSelection",
        "key": "ctrl+alt+p",
        "when": "editorTextFocus"
      },
      {
        "command": "zanza.startGitBash",
        "key": "ctrl+shift+f9"
      },
      {
        "command": "zanza.startTerminal",
        "key": "ctrl+f9"
      },
      {
        "command": "editor.action.deleteLines",
        "key": "alt+backspace",
        "when": "textInputFocus && !editorReadonly"
      },
      {
        "command": "workbench.action.editorLayoutSingle",
        "key": "ctrl+alt+\\"
      },
      {
        "command": "workbench.files.action.collapseExplorerFolders",
        "key": "shift+left",
        "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
      },
      {
        "command": "editor.action.blockComment",
        "key": "ctrl+alt+/",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "workbench.action.terminal.focusPrevious",
        "key": "ctrl+alt+left",
        "when": "terminalFocus && terminalProcessSupported",
        "mac": "ctrl+shift+left"
      },
      {
        "command": "workbench.action.terminal.focusNext",
        "key": "ctrl+alt+right",
        "when": "terminalFocus && terminalProcessSupported",
        "mac": "ctrl+shift+right"
      },
      {
        "command": "explorer.newFile",
        "key": "alt+insert"
      },
      {
        "command": "explorer.newFolder",
        "key": "shift+alt+insert"
      },
      {
        "command": "editor.emmet.action.wrapWithAbbreviation",
        "key": "shift+alt+t"
      },
      {
        "command": "editor.action.joinLines",
        "key": "ctrl+shift+j",
        "when": "editorTextFocus",
        "mac": "cmd+shift+j"
      },
      {
        "command": "workbench.action.closeOtherEditors",
        "key": "ctrl+k shift+w",
        "mac": "cmd+k shift+w"
      },
      {
        "command": "workbench.action.toggleTabsVisibility",
        "key": "ctrl+alt+t"
      },
      {
        "command": "workbench.action.toggleMaximizedPanel",
        "key": "ctrl+alt+up",
        "when": "terminalFocus"
      },
      {
        "command": "editor.action.insertSnippet",
        "key": "shift+space",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "typescript.restartTsServer",
        "key": "ctrl+shift+alt+r"
      }
    ],
