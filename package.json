    "menus": {
      "explorer/context": [
        {
          "command": "zanza.openFolderNewInstance",
          "group": "navigation@100",
          "when": "explorerResourceIsFolder"
        }
      ],
      "commandPalette": [
        {
          "command": "zanza.openFolderNewInstance",
          "when": "never"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "build": "node scripts/build.js && vsce package -o build",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "lint:recommended": "eslint --config=.eslintrc-recommended.json src --ext ts",
    "format": "prettier -w src",
    "test": "prettier -c src && eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/node": "14.x",
    "@types/vscode": "1.59.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "@vscode/vsce": "2.29.0",
    "eslint": "7.27.0",
    "glob": "7.1.7",
    "prettier": "2.3.2",
    "ts-node": "10.2.1",
    "typescript": "4.3.2",
    "vscode-test": "1.5.2"
  }
